<div class="row">
  {{#each workout.exercises}}
  <div class="panel small-12 columns">
    <h3>{{name}}</h3>
    {{#each sets}}
      <button on-click="updateProgress" class="button expand {{status}}">
        {{actual_repetitions}} x {{target_repetitions}} @ {{target_weight}} lbs
      </button>
    {{/each}}
  </div>
  {{/each}}
</div>
