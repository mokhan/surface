<div class="row">
  {{#each training_session.exercises}}
  <div class="panel small-12 columns">
    <h3>{{name}}</h3>
    {{#each sets}}
      <button on-click="updateProgress" class="button small round {{status}}">
        {{actual_repetitions}}/{{target_repetitions}} @ {{target_weight}} lb
      </button>
    {{/each}}
  </div>
  {{/each}}
</div>
